[root@control01 opc]# cat instance_to_error.py 
import  sys
import imp

if len(sys.argv) < 2:
    raise Exception("You must specify an instance name as an argument.")

instance_name = sys.argv[1]

print "Instance: %s" % instance_name

bsitedb = imp.load_source('bsitedb', '/usr/lib/nimbula/bsitedb')
config = bsitedb.get_config('bsite')

from bsite.models import Instance
Instance._transport = bsitedb.get_transport(config)

ins = Instance.get(name=instance_name)
ins.state = u'error'
ins.save()
[root@control01 opc]#


#!/bin/bash
echo "Checking compute objects for account $1"
echo  "Instances: "
nimbula-api list instance -fcsv -Favailability_domain,state,name $1 | grep /  | cut -d, -f-2 | sort | uniq -c
echo "Orchestrations v1: "
nimbula-api list orchestration -fcsv -Fstatus,name $1 | grep / | cut -d, -f1 | sort | uniq -c
echo "Orchestrations v2: "
nimbula-api list orchestrationv2 -fcsv -Fstatus,name $1 | grep / | cut -d, -f1 | sort | uniq -c
echo  "Storagevols: "
nimbula-api list storagevolume -fcsv -Favailability_domain,status,name $1 | grep / | cut -d, -f-2 | sort | uniq -c
echo  "Storage snapshots: "
nimbula-api list storagesnapshot -fcsv -Fstatus,name $1 | grep / | cut -d, -f1 | sort | uniq -c
echo  "IP reservations , flat: "
nimbula-api list ipreservation -fcsv -Fused,parentpool,name $1 | grep / | cut -d, -f-2 | sort | uniq -c
echo  "IP reservations , Higgs: "
nimbula-api list ipaddressreservation -fcsv -FipAddressPool,name $1 | grep / | cut -d, -f1 | sort | uniq -c
echo  "Databag: "
nimbula-api list databag -fcsv -Fstatus,name $1 | grep / | cut -d/ -f-2 | sort | uniq -c
